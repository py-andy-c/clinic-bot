{
  "name": "clinic-bot-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate-types": "openapi-typescript http://localhost:8000/openapi.json --output src/types/api.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:basic": "E2E_SKIP_BACKEND=1 NODE_ENV=test playwright test basic-test.spec.ts",
    "test:e2e:check": "playwright test playwright-check.spec.ts --config=playwright-no-server.config.ts",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:auth": "playwright test --grep '@auth'",
    "test:e2e:settings": "playwright test --grep '@settings'",
    "test:e2e:calendar": "playwright test --grep '@calendar'",
    "test:e2e:clinic": "playwright test --grep '@clinic'",
    "test:e2e:basic-tag": "playwright test --grep '@basic'",
    "test:e2e:changed": "node scripts/run-changed-e2e-tests.js",
    "test:e2e:quick": "npm run test:e2e:check && npm run test:e2e:basic-tag"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@line/liff": "^2.27.2",
    "@types/react": "^18.3.3",
    "@types/react-big-calendar": "^1.16.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "axios": "^1.6.8",
    "axios-retry": "^4.5.0",
    "clsx": "^2.1.1",
    "i18next": "^25.6.3",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "postcss": "^8.4.38",
    "react": "^18.3.1",
    "react-big-calendar": "^1.19.4",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.68.0",
    "react-i18next": "^16.3.5",
    "react-router-dom": "^6.23.1",
    "recharts": "^3.5.1",
    "tailwindcss": "^3.4.3",
    "vite": "^5.3.1",
    "zod": "^3.22.4",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.19.24",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "@vitest/ui": "^4.0.6",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "jsdom": "^27.1.0",
    "openapi-typescript": "^6.7.5",
    "typescript": "^5.2.2",
    "vitest": "^4.0.6"
  }
}
